# Aptos Configuration
NEXT_PUBLIC_APP_NETWORK=testnet
NEXT_PUBLIC_APTOS_API_KEY=your_aptos_api_key_here
NEXT_PUBLIC_MODULE_ADDRESS=0x90b34a9b1c27e9f299a004f1878bc6ccef137ff6542790876c04f6a95db9e377

# Module Publisher (for contract deployment)
NEXT_MODULE_PUBLISHER_ACCOUNT_ADDRESS=your_publisher_address
NEXT_MODULE_PUBLISHER_ACCOUNT_PRIVATE_KEY=your_private_key

# Optional: Custom Node URL (if not using default)
NEXT_PUBLIC_NODE_URL=https://fullnode.testnet.aptoslabs.com/v1

# Analytics & Monitoring (Optional)
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ANALYTICS_API_KEY=your_analytics_key

# Sentry Error Tracking (Optional)
SENTRY_DSN=your_sentry_dsn